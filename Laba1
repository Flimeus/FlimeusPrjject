using System;

namespace Lab1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.Write("Введите R1 и нажмите Enter: "); int R1 = Convert.ToInt32(Console.ReadLine());
            Console.Write("Введите R2 и нажмите Enter: "); int R2 = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("Ответ по формуле нахождения сопротивления R1*R2%R1+R2");
            Console.WriteLine((R1 * R2)/(R1 + R2));
        }
    }
}
